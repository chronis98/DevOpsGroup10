{
    "secrets":[
        {
            "name": "DATABASE_PASSWORD",
            "valueFrom": "arn:aws:secretsmanager:us-east-1:528775740130:secret:password"
        }
    ],
    "containerDefinitions": [
        {
            "name": "api",
            "image": "528775740130.dkr.ecr.us-east-1.amazonaws.com/gymcheck/api:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 8000,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "API_PORT",
                    "value": "8000"
                },
                {
                    "name": "MYSQL_HOST",
                    "value": "gymcheck-rds-db.c326kshsg1xs.eu-west-2.rds.amazonaws.com"
                },
                {
                    "name": "MYSQL_USER",
                    "value": "admin"
                },
                {
                    "name": "MYSQL_PASSWORD",
                    "value": "$DATABASE_PASSWORD"
                },
                {
                    "name": "MYSQL_DATABASE",
                    "value": "gymcheck"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
